<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Login Response</title>
        
        <meta charset="utf-8">
        <meta name="description" content="LTI 1.3 Tool">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    
        <base href="/">
    </head>
    <body>
        <form id="initLoginForm" name="initLoginForm" action=<%= action %> method="GET">
            <% Object.keys(loginResParams).forEach(function(key) { %>
                <input name=<%= key %> value=<%= loginResParams[key] %> type="hidden">
            <% }); %>
            <input name="commit" value="Post request" type="hidden">
        </form>
        <script>
            document.getElementById("initLoginForm").submit();
        </script>
    </body>
</html>